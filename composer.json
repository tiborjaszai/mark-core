{
    "name": "tiborjaszai/mark-core",
    "description": "Core package for the \"Mark\" project. Mark is a simple static site generator.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tibor Jaszai",
            "email": "tiborjaszaidev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "JTG\\Mark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JTG\\Mark\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/yaml": "^6.1",
        "symfony/config": "^6.1",
        "twig/twig": "^3.4",
        "league/commonmark": "^2.3",
        "symfony/finder": "^6.1",
        "symfony/filesystem": "^6.1",
        "symfony/console": "^6.1",
        "jms/serializer": "^3.18",
        "symfony/string": "^6.1",
        "symfony/var-dumper": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/routing": "^6.1",
        "symfony/event-dispatcher": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    }
}
